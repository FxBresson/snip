{
  "name": "@snip/cli",
  "description": "A CLI tool for managing and sharing code snippets across projects and teams",
  "version": "0.1.0",
  "author": "FranÃ§ois-Xavier Bresson",
  "license": "UNLICENSED",
  "bin": {
    "snip": "./bin/run.js"
  },
  "main": "dist/index.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "repository": "FxBresson/snip",
  "homepage": "https://github.com/FxBresson/snip",
  "bugs": "https://github.com/FxBresson/snip/issues",
  "type": "module",
  "types": "dist/index.d.ts",
  "scripts": {
    "build:link": "yarn build && npm link",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "keywords": [
    "oclif"
  ],
  "oclif": {
    "bin": "snip",
    "dirname": "snip",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "chalk": "^5.4.1",
    "clipboardy": "^4.0.0",
    "fs-extra": "^11.3.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^12.8.2",
    "ora": "^8.2.0",
    "simple-git": "^3.28.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4.1.13",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.2",
    "@types/chai": "^5.2.2",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.8",
    "@types/mocha": "^10",
    "@types/node": "^24.3.0",
    "@types/sinon": "^17.0.4",
    "chai": "^5.3.1",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^11.7.1",
    "oclif": "^4",
    "semantic-release": "^24.2.7",
    "shx": "^0.4.0",
    "sinon": "^21.0.0",
    "ts-node": "^10",
    "typescript": "^5"
  }
}